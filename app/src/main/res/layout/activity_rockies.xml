<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Main" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rockimageView2"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/p55"
        android:background="#ff59bcff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/rockies_title"
        android:id="@+id/rocktextView9"
        android:autoText="true"
        android:textColor="#ff122d44"
        android:layout_below="@+id/rockimageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/rockies_des"
        android:id="@+id/rocktextView10"
        android:layout_below="@+id/rocktextView9"
        android:layout_alignLeft="@+id/rocktextView9"
        android:layout_alignStart="@+id/rocktextView9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rockimageButton9"
        android:src="@drawable/mapbutton"
        android:layout_alignTop="@+id/rockimageButton10"
        android:layout_alignLeft="@+id/rocktextView10"
        android:layout_alignStart="@+id/rocktextView10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rockimageButton10"
        android:src="@drawable/webbutton"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rockimageButton11"
        android:src="@drawable/commend"
        android:layout_alignTop="@+id/rockimageButton10"
        android:layout_alignRight="@+id/rocktextView10"
        android:layout_alignEnd="@+id/rocktextView10" />


</RelativeLayout>