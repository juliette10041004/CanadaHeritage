<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Main" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albertaimageView2"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/p33"
        android:background="#ff59bcff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/alberta_title"
        android:id="@+id/albertatextView9"
        android:autoText="true"
        android:textColor="#ff122d44"
        android:layout_below="@+id/albertaimageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/alberta_des"
        android:id="@+id/albertatextView10"
        android:layout_below="@+id/albertatextView9"
        android:layout_alignLeft="@+id/albertatextView9"
        android:layout_alignStart="@+id/albertatextView9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albertaimageButton9"
        android:src="@drawable/mapbutton"
        android:layout_alignTop="@+id/albertaimageButton10"
        android:layout_alignLeft="@+id/albertatextView10"
        android:layout_alignStart="@+id/albartatextView10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albertaimageButton10"
        android:src="@drawable/webbutton"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/albertaimageButton11"
        android:src="@drawable/commend"
        android:layout_alignTop="@+id/albertaimageButton10"
        android:layout_alignRight="@+id/albertatextView10"
        android:layout_alignEnd="@+id/albertatextView10" />


</RelativeLayout>